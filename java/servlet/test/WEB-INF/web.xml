<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="3.1">
  <servlet>
    <servlet-name>Index</servlet-name>
    <servlet-class>Index</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/homepage-progetto</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>Register</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>HandleGrade</servlet-name>
    <servlet-class>HandleGrade</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HandleGrade</servlet-name>
    <url-pattern>/handle_grade</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>homepage-progetto</welcome-file>
  </welcome-file-list>
</web-app>
